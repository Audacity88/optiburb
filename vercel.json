{
    "version": 2,
    "builds": [
        {
            "src": "web/app.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.9",
                "installCommand": "apt-get update && apt-get install -y libgeos-dev && pip install -r requirements.txt"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/"
        }
    ],
    "env": {
        "PYTHONPATH": "."
    }
} 